	EnableSequence
		'MultiMedia1 Mixer INTERNAL_FM_TX':1:1
	EndSequence

	DisableSequence
		'MultiMedia1 Mixer INTERNAL_FM_TX':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 0
	PlaybackPCM 0

EndSection

SectionVerb
